{
    "components": {
        "responses": {
            "400": {
                "description": "The request is invalid.",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "Data must be a JSON object. Please provide a valid JSON object."
                                }
                            }
                        }
                    }
                }
            },
            "401": {
                "description": "Unauthorized. Invalid or missing API key.",
