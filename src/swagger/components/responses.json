                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "API key is required. Please provide a valid API key in the X-API-Key header."
                                }
                            }
                        }
                    }
                }
            },
            "409": {
                "description": "A document with the provided ID already exists in the specified bucket.",
