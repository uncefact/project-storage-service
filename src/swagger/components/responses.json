                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "A document with the provided ID already exists in the specified bucket."
                                }
                            }
                        }
                    }
                }
            },
            "404": {
                "description": "The requested resource was not found.",
